<h1>
    Countries
</h1>

<button onclick="window.location.href='/addCountry'">Add Country</button>

<table border="1">
    <tr>
    <th>Country Code</th>
    <th>Country Name</th>
    <th>Country Details</th>
    <th>Action</th>
    </tr>
    <% countries.forEach((country) => { %>
    <tr>
    <td><%= country.co_code %></td>
    <td><%= country.co_name %></td>
    <td><%= country.co_details %></td>
    <td><a href="/">Update</a></td>
    <td><a  href="/delete/<%= country.co_code %>" >Delete</a></td>
    <td><%=  %></td>
    </tr>
    <% }) %>
</table>

<a href="/">Home</a>