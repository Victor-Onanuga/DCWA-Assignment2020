<h1>
    Cities
</h1>

<div id="tableDiv" style="margin-top: 40px">
    <table border="1">
        <tr>
        <th>City Code</th>
        <th>Country Code</th>
        <th>City Name</th>
        <th>Population</th>
        <th>By the Sea</th>
        <th>Area</th>
        <th></th>
        </tr>
        <% cities.forEach((city) => { %>
        <tr>
        <td><%= city.cty_code %></td>
        <td><%= city.co_code %></td>
        <td><%= city.cty_name %></td>
        <td><%= city.population %></td>
        <td><%= city.isCoastal %></td>
        <td><%= city.areaKM %></td>
        <td><a href="/cities/?">All Details</a></td>
        </tr>
        <% }) %>
        </table>
</div>


