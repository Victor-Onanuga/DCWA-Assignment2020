<h1>
    Heads Of State
</h1>

<button onclick="window.location.href='/addHeadOfState'">Add Head of State</button>

<table border="1">
    <tr>
    <th>Country Code</th>
    <th>Head of State</th>
    </tr>
    <% StateHeads.forEach((headOfState) => { %>
    <tr>
    <td><%= headOfState._id %></td>
    <td><%= headOfState.headOfState %></td>
    </tr>
    <% }) %>
</table>

<a href="/">Home</a>